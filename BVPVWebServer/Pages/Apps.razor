@page "/apps"

@inject StateService _state
@inject NavigationManager _navMgr
@{
    if (_state.UserId != Environment.UserName)
    {
        _state.InitUserInfo(Environment.UserName);
    }
}
@{
    if (!_state.IsLoggedIn)
    {
        _navMgr.NavigateTo("/");
    }
}

